<?xml version="1.0" encoding="utf-8"?>
<preference-headers
  xmlns:android="http://schemas.android.com/apk/res/android">
    <header android:id="@id/assistant_category" android:title="@string/assistant_header_category_title" />
    <header android:id="@id/assistant_settings_header" android:title="@string/assistant_settings_header_title">
        <extra android:name="ASSISTANT_SETTINGS_FEATURE_KEY" android:value="main_menu" />
        <intent android:targetPackage="com.google.android.googlequicksearchbox" android:targetClass="com.google.android.apps.gsa.assistant.settings.AssistantSettingsActivity" />
    </header>
    <header android:id="@id/what_can_you_do_header" android:title="@string/assistant_settings_help_title">
        <extra android:name="ASSISTANT_SETTINGS_FEATURE_KEY" android:value="help_menu" />
        <intent android:targetPackage="com.google.android.googlequicksearchbox" android:targetClass="com.google.android.apps.gsa.assistant.settings.AssistantSettingsActivity" />
    </header>
    <header android:id="@id/explore_header" android:title="@string/assistant_settings_hq_explore_header">
        <extra android:name="section" android:value="explore" />
        <intent android:targetPackage="com.google.android.googlequicksearchbox" android:targetClass="com.google.android.apps.gsa.staticplugins.opa.hq.OpaHqActivity" />
    </header>
    <header android:id="@id/your_stuff_header" android:title="@string/assistant_settings_hq_yourstuff_header">
        <extra android:name="section" android:value="your_stuff" />
        <intent android:targetPackage="com.google.android.googlequicksearchbox" android:targetClass="com.google.android.apps.gsa.staticplugins.opa.hq.OpaHqActivity" />
    </header>
</preference-headers>